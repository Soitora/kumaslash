  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/ghostbear/kumaslash/issues"
  },
  "homepage": "https://github.com/ghostbear/kumaslash#readme",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/node": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.2",
    "axios": "^0.24.0",
    "discord.js": "^13.4.0",
